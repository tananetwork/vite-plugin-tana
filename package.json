{
  "name": "@tananetwork/vite-plugin-tana",
  "version": "0.1.1",
  "description": "Vite plugin for building full-stack applications on the Tana blockchain platform",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "tana-build": "./dist/build.js"
  },
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./build": {
      "import": "./dist/build.js",
      "types": "./dist/build.d.ts"
    }
  },
  "files": [
    "dist",
    "templates"
  ],
  "scripts": {
    "dev": "tsup --watch",
    "build": "tsup",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "start": "mprocs",
    "demo": "cd templates/default && npm run dev"
  },
  "keywords": [
    "vite",
    "vite-plugin",
    "tana",
    "blockchain",
    "ssr",
    "react"
  ],
  "author": "Tana Network",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/tananetwork/vite-plugin-tana"
  },
  "peerDependencies": {
    "vite": "^5.0.0"
  },
  "dependencies": {
    "esbuild": "^0.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "optionalDependencies": {
    "@tananetwork/tana-darwin-arm64": "^0.2.0",
    "@tananetwork/tana-darwin-x64": "^0.2.0",
    "@tananetwork/tana-linux-x64": "^0.2.0",
    "@tananetwork/tana-linux-arm64": "^0.2.0",
    "@tananetwork/tana-windows-x64": "^0.2.0"
  }
}
